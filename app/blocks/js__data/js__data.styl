.js__data
	display block
	margin-top 35px
	margin-left 8px
@media screen and (min-width: 1400px)
	.js__data
		margin-left 0px
.title__jsskill
	font-set(24px,700,49.83px)
	font-family "PT Sans"
	
.slider__skill
	margin-top 2%
	
.RangeSlider_ClickArea {
	position: relative;
	padding: 35px 3px 21px 0px;
	margin-top: -20px;
	user-select: none;
}

.RangeSlider_Track {
	border: 1px solid #5e5e5e;
	background-color: #2f1b15;
	position: relative;
	transition: all .3s;
	width:777px;
}
.RangeSlider_TrackFill {
	position: relative;
	width: 0%;
	height: 1px;
	transition: width .1s ease-in;
}

.RangeSlider_TrackFill.RangeSlider_TrackFill-stopAnimation {
	transition: none;
}
.RangeSlider_Thumb {
	sprite $cursor
	position: absolute;
	top: 0;
	right: 0;
	border-radius: 50%;
	cursor: pointer;
	transform: translate(50%, -120.33%);
}

.RangeSlider_TrackFill-stopAnimation .RangeSlider_Thumb:before {
	content: "";
	display: block;
	width: 700px;
	height: 1000px;
	position: absolute;
	top: -500px;
	left: -500px;
	border-radius: 50%;
	/* background: rgba(255,255,255,0.1); */
}
.RangeSlider_Points {
	display: flex;
	width: 770px;
}
.RangeSlider_Point  > p
	font-size: 11px;
	color: #ddd;
	cursor: pointer;
	font-set(13px,700,19px)
	font-family "PT Sans"

	
.RangeSlider_Point:hover {
	color: #fff;
}
.RangeSlider_Point:before {
	content: ""
	display: block
	background-color: #666
	margin: 0 auto
	width: 3px
	height: 26px
	top: -1px
	left: -1px
	position: relative
}
.RangeSlider_Point.RangeSlider_PointActive {
	color: #FFDF1B;
}

.RangeSlider_Point:first-child
	flex: 1.12 1 0%
	position: relative
	right: 67.6px
	
.RangeSlider_Point:last-child
	flex: 1 1 0
	position: relative
	left: 70.6px
	
.RangeSlider_Point:nth-child(2),
.RangeSlider_Point:nth-child(3)
	flex 2 1 0%
	
.RangeSlider_Point:nth-child(2):before,
.RangeSlider_Point:nth-child(3):before
					height 11px
					
					
.RangeSlider_Point:nth-child(2) > p,
.RangeSlider_Point:nth-child(3) > p
				margin-top 13.5px
	
.RangeSlider_Point:nth-child(2)::before
	left -6px
	
.RangeSlider_Point:nth-child(2) p
	width: 80%
	text-align: left
	padding-left: 5px
	
.RangeSlider_Point:nth-child(1) p
	width: 100%
	text-align: center
	padding-left: 60px
	
.RangeSlider_Point:nth-child(3) p
	margin-left -16px
	width 70%
	
.RangeSlider_Point:nth-child(4) p
	margin-left: -78px
	margin-top: 5px
